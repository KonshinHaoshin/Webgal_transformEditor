{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "transformeditor",
  "version": "1.0.2",
  "identifier": "com.administrator.transformeditor",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1421",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "WebGAL运镜编辑器",
        "width": 1024,
        "height": 768,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "log": null,
    "opener": null
  }
}
